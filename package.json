{
  "name": "@conorroberts/utils",
  "description": "",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsc --noEmit && tsup",
    "build:clean": "tsc --noEmit && tsup --clean"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    "./db": {
      "types": "./dist/db.d.ts",
      "default": "./dist/db.js",
      "require": "./dist/db.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "default": "./dist/logger.js",
      "require": "./dist/logger.cjs"
    },
    "./cache": {
      "types": "./dist/cache.d.ts",
      "default": "./dist/cache.js",
      "require": "./dist/cache.cjs"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./dist/env.js",
      "require": "./dist/env.cjs"
    },
    "./migrate": {
      "types": "./dist/migrate.d.ts",
      "default": "./dist/migrate.js",
      "require": "./dist/migrate.cjs"
    },
    "./images": {
      "types": "./dist/images.d.ts",
      "default": "./dist/images.js",
      "require": "./dist/images.cjs"
    }
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@logtail/pino": "^0.4.17",
    "@paralleldrive/cuid2": "^2.2.2",
    "dayjs": "^1.11.11",
    "drizzle-orm": "^0.31.0",
    "ioredis": "^5.4.1",
    "ofetch": "^1.3.4",
    "pino": "^9.1.0",
    "pino-pretty": "^10.3.1",
    "remeda": "^2.0.10",
    "unstorage": "^1.10.2",
    "valibot": "0.37.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  },
  "version": "0.0.12"
}
