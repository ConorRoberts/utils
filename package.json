{
  "name": "@conorroberts/utils",
  "description": "",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsdown --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "build": "tsc --noEmit && tsdown",
    "build:clean": "tsc --noEmit && tsdown --clean"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    "./env": {
      "types": "./dist/env.d.mts",
      "default": "./dist/env.mjs"
    },
    "./images": {
      "types": "./dist/images.d.mts",
      "default": "./dist/images.mjs"
    },
    "./react": {
      "types": "./dist/react.d.mts",
      "default": "./dist/react.mjs"
    },
    "./oxlint/config": {
      "default": "./dist/oxlint/config.mjson"
    },
    "./oxlint": {
      "types": "./dist/oxlint/index.d.mts",
      "default": "./dist/oxlint/index.mjs"
    }
  },
  "dependencies": {
    "dayjs": "1.11.19",
    "nanoid": "5.1.6",
    "ofetch": "1.5.1",
    "oxlint": "1.35.0",
    "remeda": "2.32.0",
    "valibot": "1.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@testing-library/react": "16.3.0",
    "@types/node": "25.0.1",
    "@types/react": "19.2.7",
    "jsdom": "27.4.0",
    "react": "19.2.3",
    "tsdown": "0.18.2",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  },
  "version": "0.0.79"
}
